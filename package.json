{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxi build --dotenv .env",
    "dev": "nuxi dev --dotenv .env.local --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "generate": "nuxi generate --dotenv .env",
    "preview": "nuxi preview --dotenv .env",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "proxy": "mkcert -install && mkcert localhost",
    "deploy": "npm run generate && gh-pages -d .output/public --nojekyll"
  },
  "devDependencies": {
    "@nuxt/image-edge": "^1.1.0-28355789.b3279fe",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/google-fonts": "^3.0.1",
    "@nuxtjs/tailwindcss": "^6.7.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18",
    "@vue/runtime-dom": "^3.3.4",
    "eslint": "^8.41.0",
    "gh-pages": "^6.1.0",
    "nuxt": "^3.5.1",
    "nuxt-headlessui": "^1.1.4",
    "stylelint": "^15.6.2",
    "stylelint-config-recommended": "^12.0.0",
    "typescript": "^5.0.4",
    "vue-tsc": "^1.6.5"
  },
  "dependencies": {
    "@headlessui/tailwindcss": "^0.1.3",
    "@heroicons/vue": "^2.0.18",
    "@storyblok/nuxt": "^5.7.9"
  }
}
